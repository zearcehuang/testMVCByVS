@using testMVC.Models
@model IEnumerable<testMVC.Models.DT311_ACode>

@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CODE_TYPE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CODE)
            </th>
            <th>@Html.DisplayNameFor(model => model.CODE_NAME)</th>
            <th>@Html.DisplayNameFor(model => model.CODE_SEQ)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            var item = Model.ToArray()[i];
            <tr>
                <td>
                    @Html.DisplayFor(modelitem => item.CODE_TYPE)
                </td>
                <td>
                    @Html.DisplayTextFor(modelitem => item.CODE)
                </td>
                <td>@Html.DisplayFor(modelitem => item.CODE_NAME)</td>
                <td>@Html.DisplayFor(modelitem => item.CODE_SEQ)</td>
                <td>@Html.ActionLink("修改", "Edit", new { id = item.CODE, codetype = item.CODE_TYPE })</td>
            </tr>
        }

    </tbody>
</table>
